{
  "name": "silverwp",
  "type": "wordpress-plugin",
  "description": "WordPress framework to help developers create plugins and themes",
  "homepage": "http://silversite.pl",
  "license": "GPLv2 or later",
  "authors": [
    {
      "name": "Michal Kalkowski",
      "homepage": "http://silversite.pl"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/reduxframework/kirki"
    },
    {
      "type": "package",
      "package": {
        "name": "reduxframework/kirki",
        "version": "0.8.4",
        "type": "library",
        "source": {
          "url": "https://github.com/reduxframework/kirki.git",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.3",
    "reduxframework/kirki": "*",
    "oncletom/wp-less":"@stable",
    "oyejorge/less.php": "~1.7"
  },
  "autoload": {
    "files": [
      "vendor/vafour/vafpress-framework/bootstrap.php",
      "vendor/evanto/auto-updates/class-pixelentity-theme-update.php",
      "vendor/oyejorge/less.php/lessc.inc.php",
      "vendor/oncletom/wp-less/bootstrap-for-theme.php"
    ],
    "psr-4": {
      "SilverWp\\": "SilverWp/"
    }
  }
}
